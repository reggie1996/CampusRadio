<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_live_room2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a24073.campusradio.controller.activity.LiveRoom2Activity">

    <com.freegeek.android.materialbanner.MaterialBanner
        android:id="@+id/material_banner_liveroom2"
        android:layout_width="match_parent"
        app:match="true"
        android:layout_height="360dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">
    </com.freegeek.android.materialbanner.MaterialBanner>

    <com.anbetter.danmuku.DanMuView
        android:id="@+id/danmaku_container_broadcast"
        android:layout_width="match_parent"
        android:layout_height="144dp"/>

    <Button
        android:text="         "
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/button10"
        android:onClick="adddanmu"
        android:background="#00000000"/>

</FrameLayout>
